@article{putluriMetabolomicProfilingReveals2011,
  title = {Metabolomic {{Profiling Reveals}} a {{Role}} for {{Androgen}} in {{Activating Amino Acid Metabolism}} and {{Methylation}} in {{Prostate Cancer Cells}}},
  author = {Putluri, Nagireddy and Shojaie, Ali and Vasu, Vihas T. and Nalluri, Srilatha and Vareed, Shaiju K. and Putluri, Vasanta and {Vivekanandan-Giri}, Anuradha and Byun, Jeman and Pennathur, Subramaniam and Sana, Theodore R. and Fischer, Steven M. and Palapattu, Ganesh S. and Creighton, Chad J. and Michailidis, George and Sreekumar, Arun},
