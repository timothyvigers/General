@article{raviPediatricMedicaidPatients2020,
  title = {Pediatric {{Medicaid Patients With Type}} 1 {{Diabetes Benefit From Continuous Glucose Monitor Technology}}},
  author = {Ravi, Sonalee J. and Coakley, Alexander and Vigers, Tim and Pyle, Laura and Forlenza, Gregory P. and Alonso, Todd},
